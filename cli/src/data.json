{
    "title": "welcome to qdev (q), a cli tool to help speed up dev setup",
    "subtitle": "type 'q help' for help",
    "commands": [
        {
            "name": "help",
            "description": "display help information"
        },
        {
            "name": "config",
            "description": "configure qdev",
            "usage": "q config <option> <value>",
            "options": [
                {
                    "name": "username",
                    "description": "set your github username",
                    "example": "q config username octocat"
                }
            ]
        },
        {
            "name": "clone",
            "description": "clone a repository from github",
            "usage": "q clone <option>",
            "options": [
                {
                    "name": "url",
                    "description": "clones a repository from a full git repository url",
                    "example": "q clone https://github.com/username/repository.git"
                }
            ]
        }
    ]
}
